{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "react-src/**/*.ts",
    "react-src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}